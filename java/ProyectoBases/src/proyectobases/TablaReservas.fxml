<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="360.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proyectobases.TablaReservasController">
   <children>
      <Label layoutX="14.0" layoutY="6.0" text="Reservas de Vuelo">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
      <TableView fx:id="TablaReserva" layoutX="16.0" layoutY="38.0" prefHeight="257.0" prefWidth="551.0">
        <columns>
          <TableColumn fx:id="colIdReserva" prefWidth="93.0" text="idReserva" />
          <TableColumn fx:id="ColIdCliente" prefWidth="95.0" text="idCliente" />
            <TableColumn fx:id="ColNumVuelos" prefWidth="106.0" text="NumeroVuelos" />
            <TableColumn fx:id="ColClase" prefWidth="111.0" text="Clase" />
            <TableColumn fx:id="ColIdPaquete" prefWidth="75.0" text="idPaquete" />
            <TableColumn fx:id="ColIdVuelo" prefWidth="70.0" text="idVuelo" />
        </columns>
      </TableView>
      <Button fx:id="botonNuevo" layoutX="16.0" layoutY="315.0" mnemonicParsing="false" onAction="#nuevaReserva" text="Nueva Reserva" />
      <Button fx:id="botonActualizar" layoutX="124.0" layoutY="315.0" mnemonicParsing="false" onAction="#actualizar" text="Actualizar Reserva" />
      <Button fx:id="botonEliminar" layoutX="248.0" layoutY="315.0" mnemonicParsing="false" onAction="#eliminar" text="Eliminar Reserva" />
      <Button fx:id="botonRegresar" layoutX="504.0" layoutY="315.0" mnemonicParsing="false" onAction="#regresar" text="Regresar" />
   </children>
</AnchorPane>
