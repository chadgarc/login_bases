<AnchorPane id="AnchorPane" prefHeight="281.0" prefWidth="212.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proyectobases.RegistrarNuevoController">
