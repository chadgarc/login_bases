      </Label>
      <ComboBox fx:id="comboBoxPaquete" layoutX="24.0" layoutY="93.0" prefWidth="150.0" />
      <Label fx:id="labelPaquete" layoutX="24.0" layoutY="63.0" text="Paquete:" />
      <Label fx:id="labelCliente" layoutX="24.0" layoutY="137.0" text="Cliente:" />
      <ComboBox fx:id="comboBoxCliente" layoutX="24.0" layoutY="164.0" prefWidth="150.0" />
      <Button fx:id="botonRegistrar" layoutX="32.0" layoutY="229.0" mnemonicParsing="false" onAction="#Registar" text="Registrar" />
      <Button fx:id="botonRegresar" layoutX="106.0" layoutY="229.0" mnemonicParsing="false" onAction="#regresar" text="Regresar" />
