<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane id="AnchorPane" prefHeight="336.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="proyectobases.CrearEmpleadoController">
   <children>
      <VBox layoutX="33.0" layoutY="35.0" prefHeight="300.0" prefWidth="235.0" spacing="20.0">
         <children>
            <TextField fx:id="textUsuario" promptText="Usuario" />
            <PasswordField fx:id="textContraseña" promptText="Contraseña" />
            <TextField fx:id="textNombre" promptText="Nombre" />
            <TextField fx:id="textApellido" promptText="Apellido" />
            <TextField fx:id="textCargo" promptText="Cargo" />
            <Button fx:id="buttonCrearUsuario" mnemonicParsing="false" onAction="#crearUsuario" text="Crear Usuario" />
         </children>
      </VBox>
   </children>
</AnchorPane>
