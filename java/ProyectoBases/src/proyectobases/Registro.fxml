<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="proyectobases.RegistroController">
   <children>
      <Button fx:id="botonRegistroNuevo" layoutX="18.0" layoutY="48.0" mnemonicParsing="false" onAction="#RegistrarNuevo" text="Registrar nuevo" />
      <TableView fx:id="TablaRegistros" layoutX="200.0" layoutY="36.0" prefHeight="345.0" prefWidth="321.0">
        <columns>
          <TableColumn fx:id="ColumnaPaqueteId" prefWidth="231.0" text="Paquete de Viaje" />
          <TableColumn fx:id="ColumnaCantReg" prefWidth="89.0" text="Registros" />
        </columns>
      </TableView>
      <Button fx:id="botonVerRegistro" layoutX="18.0" layoutY="85.0" mnemonicParsing="false" onAction="#VerRegistros" text="Ver Registros" />
      <Label fx:id="labelRegistro" layoutX="21.0" layoutY="14.0" text="Registros a Viajes">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="botonRegresar" layoutX="18.0" layoutY="356.0" mnemonicParsing="false" onAction="#Regresar" text="Regresar" />
   </children>
</AnchorPane>
