<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proyectobases.TablaClientesController">
   <children>
      <Label fx:id="LabelCliente" layoutX="14.0" layoutY="14.0" text="Clientes" underline="true">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
      <TableView fx:id="TablaClientes" layoutX="14.0" layoutY="40.0" prefHeight="284.0" prefWidth="568.0">
        <columns>
          <TableColumn fx:id="ColumnaId" prefWidth="97.0" text="Id" />
          <TableColumn fx:id="ColumnaNombre" prefWidth="167.0" text="Nombre" />
            <TableColumn fx:id="ColumnaApellido" prefWidth="157.0" text="Apellido" />
            <TableColumn fx:id="ColumnaFecha" minWidth="0.0" prefWidth="146.0" text="Fecha de Nacimiento" />
        </columns>
      </TableView>
      <Button fx:id="BotonCrearCliente" layoutX="33.0" layoutY="346.0" mnemonicParsing="false" onAction="#CrearCliente" text="Crear Nuevo Cliente" />
      <Button fx:id="BotonEliminarCliente" layoutX="199.0" layoutY="346.0" mnemonicParsing="false" onAction="#EliminarCliente" text="Eliminar Cliente" />
      <Button fx:id="BotonActualizarCliente" layoutX="337.0" layoutY="346.0" mnemonicParsing="false" onAction="#ActualizarCliente" text="Actualizar Cliente" />
      <Button fx:id="BotonRegresar" layoutX="519.0" layoutY="346.0" mnemonicParsing="false" onAction="#regresar" text="Regresar" />
      <Button fx:id="BotonAgregarPaquete" layoutX="423.0" layoutY="7.0" mnemonicParsing="false" onAction="#regresar" prefHeight="27.0" prefWidth="159.0" text="Agregar Paquete" />
   </children>
</AnchorPane>
